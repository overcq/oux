Pc device;
I file_id;
//==============================================================================
int
main(
  int argc
, char *argv[]
){  E_base_M( argc, argv );
    device = 0;
    for( int i = 1; i != argc; i++ )
    {   if( *argv[i] == '-' )
            goto Error;
        if(device)
            goto Error;
        device = argv[i];
    }
    if( !device )
        goto Error;
    if( !~( file_id = E_mem_Q_file_M(device) ))
        goto Error;
    if( E_mem_Q_file_P_s( file_id, "\xeb\x76\x90", 3 ))
        goto Error;
    if( E_mem_Q_file_P_s( file_id, "OUX", 3 ))
        goto Error;
    if( E_mem_Q_file_P_c( file_id, 8 )) //CONF
        goto Error;
    if( E_mem_Q_file_P_64( file_id, 0 )) ///end of table
        goto Error;
    if( E_mem_Q_file_W( file_id ))
        goto Error;
    E_base_W();
    return 0;
Error:
    E_base_W();
    return 1;
}
